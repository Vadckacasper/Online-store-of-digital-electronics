@model IEnumerable<Online_store_of_digital_electronics.Models.Products>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "ProductIndex";
}

<link rel="stylesheet" href="~/css/null.css" type="text/css" />
<link rel="stylesheet" href="~/css/bootstrap.min.css" type="text/css" />
<script src="~/js/bootstrap.min.js"></script>

<div class="row mt-5 mb-2">
    @foreach (Products item in Model)
    {
        @await Html.PartialAsync("~/Views/Shared/_AllProducts.cshtml", item)
    }
</div>